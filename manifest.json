{
  "name": "chrome extension example",
  "version": "1.0",
  "description": "example",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "webNavigation",
    "storage",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "host_permissions": [
    "*://learn.hoseo.ac.kr/"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts":[
    {
      "matches":["https://learn.hoseo.ac.kr/calendar/*"],
      "js":["jquery-3.7.1.min.js",
      "content.js"]
    }
  ],
  "background":{
    "service_worker": "background.js"
  },
  "manifest_version": 3
}